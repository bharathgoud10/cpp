int a[]= {1,2,2,2,3,4,5,5};
            	int n=a.length;
            	int t[]=new int[n];
            	int j=0;
            	for(int i=0;i<n-1;i++) {
            		if(a[i]!=a[i+1]) {
            			t[j++]=a[i];
            	      	
            		}
            	}
            	t[j++]=a[n-1];
            	
         	for(int i=0;i<j;i++) 
           		System.out.print(t[i]+" ");
